{
	"extends": [
		"./node_modules/gts/",
		"eslint:recommended",
		"plugin:@typescript-eslint/eslint-recommended",
		"plugin:@typescript-eslint/recommended",
		"prettier"
	],
	"rules": {
		"prettier/prettier": [
			"error",
			{
				"endOfLine": "lf"
			}
		],
		"node/no-unpublished-require": ["error", {"devDependencies": true}]
	}
}
