{
	"name": "mamba-ui",
	"version": "0.2.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"build:prod": "ng build --prod --build-optimizer --output-hashing all",
		"test": "ng test --browser ChromeHeadless --single-run",
		"lint": "gts lint",
		"watch": "ng test --browser ChromeHeadless",
		"e2e": "ng e2e",
		"release": "standard-version && git push --follow-tags origin master && npm publish",
		"analyze": "ng build --prod --stats-json && webpack-bundle-analyzer dist/mamba-ui/stats.json",
		"lhci": "lhci collect && lhci assert --config=./.lighthouserc.js"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~10.1.0",
		"@angular/common": "~10.1.0",
		"@angular/compiler": "~10.1.0",
		"@angular/core": "~10.1.0",
		"@angular/forms": "~10.1.0",
		"@angular/platform-browser": "~10.1.0",
		"@angular/platform-browser-dynamic": "~10.1.0",
		"@angular/router": "~10.1.0",
		"@angular/service-worker": "~10.1.0",
		"rxjs": "~6.6.0",
		"tslib": "^2.0.0",
		"zone.js": "~0.10.2"
	},
	"devDependencies": {
		"@angular-builders/custom-webpack": "^10.0.1",
		"@angular-devkit/build-angular": "^0.1100.5",
		"@angular-eslint/builder": "0.5.0-beta.4",
		"@angular-eslint/eslint-plugin": "0.5.0-beta.4",
		"@angular-eslint/eslint-plugin-template": "0.5.0-beta.4",
		"@angular-eslint/schematics": "^0.5.0-beta.4",
		"@angular-eslint/template-parser": "0.5.0-beta.4",
		"@angular/cli": "~10.1.0",
		"@angular/compiler-cli": "~10.1.0",
		"@fullhuman/postcss-purgecss": "^3.0.0",
		"@tailwindcss/forms": "^0.3.3",
		"@tailwindcss/typography": "^0.4.1",
		"@types/jasmine": "~3.5.0",
		"@types/jasminewd2": "~2.0.3",
		"@types/node": "^12.19.12",
		"@types/prismjs": "^1.16.2",
		"@typescript-eslint/eslint-plugin": "4.3.0",
		"@typescript-eslint/parser": "4.3.0",
		"autoprefixer": "^10.3.4",
		"eslint": "^7.6.0",
		"eslint-config-prettier": "^6.13.0",
		"eslint-plugin-prettier": "^3.1.4",
		"gts": "^3.1.0",
		"jasmine-core": "~3.6.0",
		"jasmine-spec-reporter": "~5.0.0",
		"karma": "~5.0.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "~3.0.2",
		"karma-jasmine": "~4.0.0",
		"karma-jasmine-html-reporter": "^1.5.0",
		"postcss": "^8.3.6",
		"postcss-import": "^12.0.1",
		"postcss-loader": "^4.0.4",
		"postcss-scss": "^3.0.2",
		"prettier": "^2.1.2",
		"prismjs": "^1.24.1",
		"protractor": "~7.0.0",
		"sass-loader": "^10.0.5",
		"standard-version": "^9.3.1",
		"tailwind-safelist-generator": "^0.1.2",
		"tailwindcss": "^2.2.9",
		"ts-node": "~8.3.0",
		"typescript": "^4.0.3",
		"webpack-bundle-analyzer": "^4.3.0"
	}
