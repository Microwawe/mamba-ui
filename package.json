{
  "name": "mamba-ui",
  "version": "0.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --configuration development",
    "build": "ng build",
    "build:dev": "ng build --configuration development --build-optimizer --output-hashing all",
    "test": "ng test",
    "test:component": "cypress run --component --quiet",
    "lint": "gts lint",
    "watch": "ng test --browser ChromeHeadless",
    "e2e": "ng e2e",
    "release": "standard-version && git push --follow-tags origin master && npm publish",
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/mamba-ui/stats.json",
    "lhci": "lhci collect && lhci assert --config=./.lighthouserc.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.5",
    "@angular/common": "^16.0.5",
    "@angular/compiler": "^16.0.5",
    "@angular/core": "^16.0.5",
    "@angular/forms": "^16.0.5",
    "@angular/platform-browser": "^16.0.5",
    "@angular/platform-browser-dynamic": "^16.0.5",
    "@angular/router": "^16.0.5",
    "@angular/service-worker": "^16.0.5",
    "rxjs": "^7.5.2",
    "tslib": "^2.3.1",
    "zone.js": "^0.13.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^13.1.0",
    "@angular-devkit/build-angular": "^16.0.5",
    "@angular-devkit/schematics": "^16.0.5",
    "@angular-eslint/builder": "^13.5.0",
    "@angular-eslint/eslint-plugin": "^13.5.0",
    "@angular-eslint/eslint-plugin-template": "^13.5.0",
    "@angular-eslint/schematics": "^16.0.3",
    "@angular-eslint/template-parser": "^13.5.0",
    "@angular/cli": "^16.0.5",
    "@angular/compiler-cli": "^16.0.5",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^17.0.45",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "autoprefixer": "^10.3.4",
    "cypress": "^12.9.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gts": "^3.1.0",
    "jasmine-core": "^4.1.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^5.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "postcss": "^8.3.6",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^4.0.4",
    "postcss-scss": "^3.0.2",
    "prettier": "^2.1.2",
    "prismjs": "^1.28.0",
    "sass-loader": "^12.1.0",
    "standard-version": "^9.3.1",
    "tailwind-safelist-generator": "^1.0.0",
    "tailwindcss": "^3.3.2",
    "ts-node": "~8.3.0",
    "typescript": "^5.0.4",
    "webpack": "^5.86.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-dev-server": "^4.9.0"
  }
}
